<h1>Listing projects</h1>

<table>
  <thead>
    <tr>
      <th>Caption</th>
      <th>Title</th>
      <th>Subtitie</th>
      <th>Thumbnail</th>
      <th>Image</th>
      <th>Description</th>
      <th>Google url</th>
      <th>Github url</th>
      <th>Music url</th>
      <th>Date</th>
      <th>Copyright</th>
      <th>Type</th>
      <th>Rank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.caption %></td>
        <td><%= project.title %></td>
        <td><%= project.subtitie %></td>
        <td><%= project.thumbnail %></td>
        <td><%= project.image %></td>
        <td><%= project.description %></td>
        <td><%= project.google_url %></td>
        <td><%= project.github_url %></td>
        <td><%= project.music_url %></td>
        <td><%= project.date %></td>
        <td><%= project.copyright %></td>
        <td><%= project.category %></td>
        <td><%= project.rank %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
